<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Meu Aplicativo ASP.NET</title>
    

  @*<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">*@
  <link rel="stylesheet" href="@Url.Content("~/Content/bootstrap.css")" />
  <link rel="stylesheet" href="@Url.Content("~/Content/bootstrap-datetimepicker.css")" />
  <link rel="stylesheet" href="@Url.Content("~/Content/site.css")" />
  
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Contact Customer", "Logoff", "Account", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                
              <ul class="nav navbar-nav navbar-right">
                <li>
                  <a href="@Url.Action("Logoff","Account")" class="btn btn-default btn-sm" style="padding-bottom: 5px;padding-top: 5px;margin-top: 9px;">

                    <span class="glyphicon glyphicon-log-out"></span> Log out

                  </a>
                </li>
              </ul>
            </div>
        </div>
    </div>
    <div class="container body-content" ng-app="contactCustomer">
        
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Meu Aplicativo ASP.NET</p>
        </footer>
    </div>
  
  <script src="@Url.Content("~/Scripts/jquery-1.10.2.min.js")"></script>
  <script src="@Url.Content("~/Scripts/moment.min.js")"></script>
  <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
  <script src="@Url.Content("~/Scripts/bootstrap-datetimepicker.min.js")"></script>
  

  <script src="@Url.Content("~/Scripts/angular.js")"></script>
  <script src="@Url.Content("~/Scripts/dirPagination.js")"></script>
  
  
    
    @RenderSection("scripts", required: false)
</body>
</html>
